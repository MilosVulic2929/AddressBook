<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se201.projekat.controllers.EditGroupController">
    <children>
        <Label layoutX="33.0" layoutY="51.0" text="Group Name:">
            <font>
                <Font name="Calibri" size="16.0" />
            </font>
        </Label>
        <TextField fx:id="textName" layoutX="175.0" layoutY="48.0" prefHeight="25.0" prefWidth="130.0" />
        <Button fx:id="btnBack" layoutX="33.0" layoutY="114.0" mnemonicParsing="false" onAction="#onBack" prefHeight="37.0" prefWidth="130.0" text="Back">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
        <Button fx:id="btnSave" layoutX="175.0" layoutY="114.0" mnemonicParsing="false" onAction="#onSave" prefHeight="37.0" prefWidth="130.0" text="Save">
            <font>
                <Font size="16.0" />
            </font>
        </Button>
    </children>

</AnchorPane>
